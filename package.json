{
  "name": "nix-sc-election-2025-voter-registration",
  "version": "0.0.0",
  "description": "Nix SC Election 2025 Voter Registration Website",
  "main": "app.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "dev": "npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch\" \"npm:dev:start\"",
    "dev:start": "npm run build && fastify start --ignore-watch=.ts$ -w -l debug -P dist/app.js",
    "lint": "eslint --ignore-pattern=dist",
    "lint:fix": "npm run lint -- --fix",
    "standalone": "npm run build && node --env-file=.env dist/server.js",
    "start": "npm run build && fastify start -l info dist/app.js",
    "test": "glob -c \"tsx --test\" \"./test/**/*.ts\"",
    "watch": "tsc -w"
  },
  "keywords": [],
  "author": "Nicolas Goudry <goudry.nicolas@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicolas-goudry/nix-sc-election-2025-voter-registration.git"
  },
  "homepage": "https://github.com/nicolas-goudry/nix-sc-election-2025-voter-registration#readme",
  "bugs": {
    "url": "https://github.com/nicolas-goudry/nix-sc-election-2025-voter-registration/issues"
  },
  "dependencies": {
    "@awesome.me/webawesome": "3.0.0-beta.6",
    "@fastify/autoload": "6.3.1",
    "@fastify/cors": "11.1.0",
    "@fastify/env": "5.0.2",
    "@fastify/formbody": "8.0.2",
    "@fastify/helmet": "13.0.1",
    "@fastify/oauth2": "8.1.2",
    "@fastify/secure-session": "8.2.0",
    "@fastify/sensible": "6.0.3",
    "@fastify/static": "8.2.0",
    "@fastify/type-provider-typebox": "5.2.0",
    "@fastify/view": "11.1.1",
    "@fontsource/inter": "5.2.8",
    "@fortawesome/fontawesome-free": "7.0.1",
    "@sinclair/typebox": "0.34.41",
    "close-with-grace": "2.2.0",
    "concurrently": "9.2.1",
    "ejs": "3.1.10",
    "fastify": "5.6.0",
    "fastify-axios": "1.3.0",
    "fastify-cli": "7.4.0",
    "fastify-plugin": "5.0.1",
    "octokit": "5.0.3",
    "papaparse": "5.5.3"
  },
  "devDependencies": {
    "@types/ejs": "3.1.5",
    "@types/node": "24.5.2",
    "@types/papaparse": "5.3.16",
    "c8": "10.1.3",
    "eslint": "9.36.0",
    "fastify-tsconfig": "3.0.0",
    "form-data": "4.0.4",
    "glob": "11.0.3",
    "neostandard": "0.12.2",
    "rimraf": "6.0.1",
    "tsx": "4.20.5",
    "typescript": "5.9.2"
  }
}
